<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <stroke android:width="1dp" android:color="#0c5b0b" />
            <gradient android:startColor="#072306" android:endColor="#0b260a" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <stroke android:width="1dp" android:color="#308c2e" />
            <solid android:color="#14ed10" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <stroke android:width="1dp" android:color="#14ed10" />
            <gradient android:startColor="#2d712c" android:endColor="#389136" android:angle="-90.0" />
        </shape>
    </item>
</selector>
